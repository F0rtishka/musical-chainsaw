class CalculatorF0rt {

    // This is a calculator
    // Made not by me

    // Addition "+"

    float addition(float x, float y) {
        return x + y;
    }

    // Substraction "-"

    float subtraction(float x, float y) {
        return x - y;
    }

    // Multiplication "*"

    float multiplication(float x, float y) {
        return x * y;
    }

    // Division "/"

    float division(float x, float y) {
        return x / y;
    }

    // Mod 

    float mod(float x, float y){
        return x % y;
    }

}